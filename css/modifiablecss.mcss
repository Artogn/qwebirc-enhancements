.channel-name {
    background-color: rgb(255, 255, 191);
    border: 1px solid #C8D1DB;
    border-radius: 4px 4px 4px 4px;
    color: #000000;
    cursor: default;
    font-size: 0.8em;
    padding: 2px;
    text-decoration: none;
    white-space: nowrap;
    float: left;
    margin: 1px 0px 0px 1px;
    font-weight: bold;
}

.qui .dynamicpanel {
  /*position: absolute;*/
}

.qui .widepanel {
    width: 100%;
}


.qui .bottompanel {
    color: red;
}

.qui .lines {
    color: black;
    overflow: auto;
    font-size: $(windowfont_size)px;
    background: $(lines_background);
}

.qui .lines .timestamp {
    display: $(timestamp_display);
}

.qui .ircwindow .lines {
    font-family: Consolas, "Lucida Console", monospace;
    text-indent: 10px;
    padding-left: $(timestamp_padding_left);
    word-wrap: break-word;
}

.qui .lines .highlight1 { /*last 5 messages not sent by the bot*/
    background-color: $(lines_highlight1);
}

.qui .lines .highlight2 { /*last 5 messages not sent by the bot*/
    background-color: $(lines_highlight2);
}

.qui .lines .highlight3 { /*last 5 messages not sent by the bot*/
    background-color: $(lines_highlight3);
}

.qui .lines .mentioned {
    background-color: $(mentioned_colour) !important;
}

.qui .properties {
    background-color: $(menu_background);
    border-top: 1px solid $(menu_border);
    height: 25px;
}

.qui .topic .emptytopic {
    color: gray;
}

.qui .topic {
    color: gray;
    padding-left: 5px;
    font-size: 0.7em;
    cursor: default;
    background-color: $(topic_background);
    border-bottom: 1px dashed $(topic_border);
}

/*tab stuff*/

.qui .outertabbar {    
    border-bottom: 1px solid $(tabbar_border);
    background: $(tabbar_background);
}

.qui .tabbar {
    font-size: 0.8em;
    color: $(tabbar_text);
    line-height: 24px;
    display: inline-block;
    /*max-width: calc(100% - 60px);*/
    overflow-x: hidden;
    margin-left: 10px;
    font-size: 13px;
}

.qui .tabbar .tab {
    border: 1px solid $(tab_border);
    padding: 2px;
    cursor: default;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    margin-right: 3px;
    white-space: nowrap;
    text-decoration: none;
    color: $(tab_text);
    font-weight: bold;
}

.qui .tabbar .tab:hover {
    background: $(tab_hover);
    border: 1px solid #c8d2dc;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
}

.qui .tabbar .tab-hilight-activity.tab {
    color: #009900;
}

.qui .tabbar .tab-hilight-speech.tab {
    color: #0000ff;
}

.qui .tabbar .tab-hilight-us.tab {
    color: #ff0000;
    background: rgb(216, 216, 138);
}

.qui .tabbar .brouhaha {
    width: 80px;
    background-image: -moz-linear-gradient(45deg, #666 25%, transparent 25%), 
        -moz-linear-gradient(-45deg, #666 25%, transparent 25%), 
        -moz-linear-gradient(45deg, transparent 75%, #666 75%), 
        -moz-linear-gradient(-45deg, transparent 75%, #666 75%);
    background-image: -webkit-gradient(linear, 0 100%, 100% 0, color-stop(.25, #666), color-stop(.25, transparent)), 
        -webkit-gradient(linear, 0 0, 100% 100%, color-stop(.25, #666), color-stop(.25, transparent)), 
        -webkit-gradient(linear, 0 100%, 100% 0, color-stop(.75, transparent), color-stop(.75, #666)), 
        -webkit-gradient(linear, 0 0, 100% 100%, color-stop(.75, transparent), color-stop(.75, #666));
    background-image: -webkit-linear-gradient(45deg, #666 25%, transparent 25%), 
        -webkit-linear-gradient(-45deg, #666 25%, transparent 25%), 
        -webkit-linear-gradient(45deg, transparent 75%, #666 75%), 
        -webkit-linear-gradient(-45deg, transparent 75%, #666 75%);
    background-image: -o-linear-gradient(45deg, #666 25%, transparent 25%), 
        -o-linear-gradient(-45deg, #666 25%, transparent 25%), 
        -o-linear-gradient(45deg, transparent 75%, #666 75%), 
        -o-linear-gradient(-45deg, transparent 75%, #666 75%);
    background-image: linear-gradient(45deg, #666 25%, transparent 25%), 
        linear-gradient(-45deg, #666 25%, transparent 25%), 
        linear-gradient(45deg, transparent 75%, #666 75%), 
        linear-gradient(-45deg, transparent 75%, #666 75%);
    -moz-background-size: 2px 2px;
    background-size: 2px 2px;
    -webkit-background-size: 2px 2.1px; /* override value for webkit */
    background-position: 0 0, 1px 0, 1px -1px, 0px 1px;
}

