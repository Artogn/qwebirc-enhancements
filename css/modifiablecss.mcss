#ircui {
  /*position: absolute;
  left: 0px;
  right: 0px;
  top: 0px;*/
    height: 100%;
    width: 100%;
    overflow: hidden;
    font-family: Verdana, sans-serif;
}

.qui .hidden, .qui .tab-invisible {
    display: none;
}

/*.qui-center {
    display: -webkit-flex;
