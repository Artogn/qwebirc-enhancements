.qui .nicklist {
    border-left: 1px solid $(nicklist_border);
  /*position: absolute;
  top: 0px;
  right: 0px;*/
    width: 140px;
    overflow: auto;
    background: $(nicklist_background);
    color: $(nicklist_text);
    font-size: 0.7em;
}

.qui .nicklist a {
    display: block;
    color: black;
    text-decoration: none;
    cursor: default;
    border-top: 1px solid $(nicklist_background);
    border-bottom: 1px solid $(nicklist_background);
    padding-left: 1px;
}

.qui .nicklist a.selected {
    display: block;
    color: black;
    background: white;
    text-decoration: none;
    border-bottom: $(nicklist_selected_border) 1px solid;
    cursor: default;
}

.qui .nicklist a.selected-middle {
    border-top: $(nicklist_selected_border) 1px solid;
}

#noscript {
    text-align: center;
    font-weight: bold;
}

.qui .nicklist .menu {
    margin: 0 0 0 5px;
}

.qui .nicklist .menu a {
    border-bottom: 0;
    border-top: 0;
}

.hyperlink-whois, .hyperlink-channel {
    cursor: pointer;
    cursor: hand;
}

.hyperlink-whois:hover, .hyperlink-channel:hover {
    text-decoration: underline;
}

.qui .outertabbar .dropdown-tab {
    cursor: pointer;
    cursor: hand;
    float: left;
    padding: 4px;
    width: 30px;
}

.qui .dropdownmenu {
    z-index: 100;
    border: 1px solid $(menu_border);
    background: $(menu_background);
}

.qui .dropdownmenu a {
    display: block;
    font-size: 0.7em;
    color: black;
    cursor: pointer;
    cursor: hand;
    padding: 1px 3px;
}

.qui .dropdownmenu a:hover {
    background: $(menu_hover_background);
 /* IE is rubbish and doesn't like #FFFFFF or white */
}

.qui .dropdownhint {
  position: relative;
  left: -500px;
  z-index: 10;
  white-space: nowrap;
  font-size: 0.7em;
}

.qui .chanmenu {
    width: 150px;
}

.qui .chanmenu .hint {
    float: right;
    font-size: 75%;
    color: grey;
}

.qui hr.lastpos {
  border: none;
  border-top: 1px solid $(lastpositionbar);
  margin-left: 3em;
  margin-right: 3em;
}

.qwebirc-init-channels {
    font-size: 95%;
    color: #928D8D;
    text-align: center;
}


/************* OPTIONS *****************/

.qwebirc-optionspane .hue-slider {
    border: 0px solid black;
    width: 360px;
    height: 8px;
    background-image: url(../images/hue.png);
    display: inline-block;
    margin-left: 15px;
}

.qwebirc-optionspane .hue-slider .knob {
    width: 8px;
    height: 16px;
    top: -5px;
    opacity: 0.75;
    background: grey;
    border: 1px solid black;
}

